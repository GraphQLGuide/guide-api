{
  "name": "guide-api",
  "version": "0.1.0",
  "description": "api.graphql.guide",
  "scripts": {
    "dev": "nodemon -e js,graphql --exec 'npm run update-graphql-imports && babel-node src/index.js'",
    "start": "node dist/index.js",
    "build": "babel src -d dist --ignore **/*.test.js",
    "update-graphql-imports": "rm -rf ./node_modules/.cache/@babel",
    "lint": "eslint src/",
    "test": "jest --coverage",
    "open-coverage": "open coverage/lcov-report/index.html"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "apollo-datasource-mongodb": "0.2.1",
    "apollo-datasource-rest": "^0.6.11",
    "apollo-server": "2.9.6",
    "apollo-server-cache-redis": "^1.1.4",
    "apollo-server-testing": "2.9.6",
    "casual": "^1.6.2",
    "datasource-sql": "^1.1.0",
    "date-fns": "2.5.1",
    "dotenv": "8.2.0",
    "graphql": "^14.5.8",
    "graphql-redis-subscriptions": "^2.1.1",
    "graphql-request": "1.8.2",
    "graphql-tools": "^4.0.6",
    "ioredis": "^4.14.1",
    "join-monster": "^2.1.1",
    "join-monster-graphql-tools-adapter": "^0.1.0",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "1.6.0",
    "knex": "^0.20.8",
    "lodash": "4.17.15",
    "mongodb": "3.3.3",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/node": "7.6.3",
    "@babel/preset-env": "7.6.3",
    "apollo-link": "1.2.13",
    "apollo-link-http": "1.5.16",
    "babel-plugin-import-graphql": "2.7.0",
    "eslint": "6.5.1",
    "eslint-plugin-node": "10.0.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "node-fetch": "2.6.0",
    "nodemon": "1.19.4"
  },
  "homepage": "https://github.com/GraphQLGuide/guide-api",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GraphQLGuide/guide-api"
  },
  "bugs": {
    "url": "https://github.com/GraphQLGuide/guide-api/issues"
  },
  "private": true,
  "author": "The GraphQL Guide <hi@graphql.guide> (https://graphql.guide)"
}
